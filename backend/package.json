{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc && npm run copy-files",
    "build:prod": "tsc -p tsconfig.prod.json && npm run copy-files",
    "copy-files": "mkdir -p dist/mailtrap && cp -r src/mailtrap/*.js dist/mailtrap/ 2>/dev/null || true",
    "start": "node dist/server.js",
    "test": "jest --testMatch='**/generateOtp.test.ts' --passWithNoTests",
    "test:all": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "lint": "eslint . --ext .ts",
    "seed": "ts-node src/scripts/seed.ts",
    "seed:edge": "ts-node src/scripts/seed-social-edge-cases.ts",
    "typecheck": "tsc --noEmit",
    "migrate:urls": "ts-node src/scripts/migrate-image-urls.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.846.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@my-app/shared": "file:../shared",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "apollo-server-express": "^3.13.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.10.1",
    "jsonwebtoken": "^9.0.2",
    "mailtrap": "^4.2.0",
    "mathpix-markdown-it": "^2.0.16",
    "mongoose": "^8.4.1",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "pdf-parse": "^1.1.1",
    "resend": "^4.7.0",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1",
    "tesseract.js-core": "^6.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-graphql": "^0.8.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.12",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "@types/tesseract.js": "^0.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "@types/express": "^4.17.21"
  }
}
